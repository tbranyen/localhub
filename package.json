{
  "name": "localhub",
  "version": "1.0.0",
  "description": "Local hub of Git repositories",
  "main": "index.js",
  "scripts": {
    "start": "node server",
    "build": "npm run cleancss && npm run browserify",
    "watch": "npm run watchify",
    "cleancss": "cleancss client/index.css -o dist/localhub.css",
    "browserify": "browserify client/index.js -o dist/localhub.js",
    "watchify": "watchify -d client/index.js -o dist/localhub.js -v"
  },
  "author": "Tim Branyen (@tbranyen)",
  "repository": "https://github.com/tbranyen/localhub",
  "license": "MIT",
  "devDependencies": {
    "babelify": "^6.3.0",
    "browserify": "^11.2.0",
    "combynexpress": "^2.0.0",
    "combynify": "^2.0.1",
    "express": "^4.13.3",
    "express-livereload": "^0.0.24",
    "falcor-express": "^0.1.2",
    "highlight.js": "^8.8.0",
    "homedir": "^0.6.0",
    "livereload": "napcs/node-livereload",
    "lodash": "^3.10.1",
    "nodegit": "^0.5.0",
    "remarkable": "^1.6.0",
    "watchify": "^3.4.0"
  },
  "dependencies": {
    "blueimp-md5": "^1.1.1",
    "bootstrap-social": "^4.9.1",
    "falcor": "^0.1.13",
    "falcor-http-datasource": "^0.1.2",
    "falcor-json-graph": "^2.0.0",
    "falcor-router": "^0.2.9",
    "font-awesome": "^4.4.0",
    "github-markdown-css": "^2.0.11",
    "jquery": "^2.1.4",
    "moment": "^2.10.6",
    "octicons": "^3.1.0",
    "primer-css": "primer/primer",
    "webapp": "webapp/library"
  },
  "browserify": {
    "transform": [
      "babelify",
      "combynify"
    ]
  }
}
